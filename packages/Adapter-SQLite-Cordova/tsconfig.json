{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "stripInternal": true,
        "removeComments": true,
        "importHelpers": false,
        "strictNullChecks": true,
        "noImplicitAny": false,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noImplicitUseStrict": true,
        "outDir": "lib",
        "declaration": true,
        "declarationDir": "lib"
    },
    "files": [
        "src/index.ts"
    ]
}